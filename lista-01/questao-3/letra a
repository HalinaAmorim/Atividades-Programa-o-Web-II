function calcular(num1, num2, operador) {
    switch (operador) {
        case '+': return num1 + num2;
        case '-': return num1 - num2;
        case '*': return num1 * num2;
        case '/': return num2 !== 0 ? num1 / num2 : "Erro: divisão por zero";
        default: return "Operador inválido";
    }
}
console.log(calcular(10, 5, '+')); 